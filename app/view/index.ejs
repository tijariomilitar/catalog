							<option value="">Cor</option>
							<% for(i in productColors){ %>
								<option value="<%= productColors[i].shortcut %>"><%= productColors[i].name %></option>
							<% } %>
						</select>
						<input type="submit" name="submit" class="box four input-generic margin-top-5 bold" style="color:#3c3c3c;" value="Filtrar">
